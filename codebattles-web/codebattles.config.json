{
    "globalHeaders": {
        "content-security-policy": "default-src https: 'unsafe-eval' 'unsafe-inline'; object-src 'none'"
    },
    "navigationFallback": {
        "rewrite": "index.html",
        "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
    },
    "responseOverrides": {     
        "404": {
          "rewrite": "/index.html",
          "statusCode": 200
        }
    },
    "mimeTypes": {
        "custom": "text/html"
    }
}
